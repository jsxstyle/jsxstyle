{
  "extends": "../tsconfig.json",
  "include": [
    "components",
    "pages",
    "next-env.d.ts"
  ],
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "skipLibCheck": true,
    "strict": true,
    "types": [
      "webpack-env"
    ],
    "noEmit": true,
    "incremental": true,
    "isolatedModules": true,
    "jsx": "preserve"
  }
}
