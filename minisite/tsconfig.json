{
  "extends": "../config/tsconfig.base.json",
  "include": [
    "components",
    "hooks",
    "pages",
    "utilities",
    "next-env.d.ts"
  ],
  "compilerOptions": {
    "rootDir": ".",
    "tsBuildInfoFile": "dist/cache.tsbuildinfo",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "skipLibCheck": true,
    "strict": true,
    "types": [
      "webpack-env"
    ],
    "noEmit": true,
    "incremental": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "target": "esnext",
    "module": "esnext"
  },
  "references": [
    {
      "path": "../packages/jsxstyle/tsconfig.json"
    }
  ]
}
