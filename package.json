{
  "name": "jsxstyle-monorepo",
  "description": "The monorepo for jsxstyle and tools built for jsxstyle",
  "license": "Apache-2.0",
  "private": true,
  "homepage": "https://github.com/jsxstyle/jsxstyle#readme",
  "scripts": {
    "start": "npm -w minisite run dev",
    "build": "npm run reset && rollup -c && prettier --write \"packages/{*,*/*}/lib/**/*.{js,ts}\"",
    "clean": "find . -name 'node_modules' -type d -prune -exec echo {} \\; -exec rm -rf {} \\;",
    "example": "node examples/run.js",
    "test": "npm run build && jest",
    "prettier": "prettier --write \"**/*.{js,ts,tsx,css,md}\"",
    "release": "changeset publish",
    "release:canary": "changeset publish --tag canary",
    "version:canary": "changeset version --snapshot canary",
    "reset": "rimraf packages/{*,*/*}/lib/",
    "update-readme": "echo \"$(<./misc/monorepo-preamble.md)\\n\\n---\\n\\n$(<./packages/jsxstyle/README.md)\" > README.md",
    "karma": "karma start",
    "karma-headless-chrome": "karma start",
    "update-sauce-data": "curl https://saucelabs.com/rest/v1/info/platforms/all > misc/saucelabs-data.json"
  },
  "workspaces": [
    "packages/*",
    "examples/*",
    "minisite"
  ],
  "prettier": {
    "singleQuote": true
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "dependencies": {
    "@babel/core": "^7.17.9",
    "@babel/generator": "^7.17.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-transform-object-assign": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/traverse": "^7.17.9",
    "@babel/types": "^7.17.0",
    "@changesets/cli": "^2.22.0",
    "@manypkg/get-packages": "^1.1.3",
    "@rollup/plugin-babel": "^6.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.1",
    "@rollup/plugin-terser": "^0.1.0",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/invariant": "^2.2.35",
    "@types/jest": "^27.4.1",
    "@types/karma": "^6.3.3",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.0.0",
    "@types/npm-packlist": "^3.0.0",
    "@types/react": "^17.0.0",
    "@types/webpack-env": "^1.16.4",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.2.5",
    "core-js": "^3.22.2",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-react": "^7.29.4",
    "inquirer": "^8.2.3",
    "invariant": "^2.2.4",
    "jasmine-core": "^4.1.0",
    "jest": "^27.1.1",
    "karma": "^6.3.19",
    "karma-chrome-launcher": "^3.1.1",
    "karma-jasmine": "^5.0.0",
    "karma-sauce-launcher": "^4.3.6",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^5.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "npm-packlist": "^5.0.2",
    "preact": "^10.7.1",
    "prettier": "^2.6.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.2.5",
    "terser": "^5.13.0",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.9",
    "ts-node": "^10.7.0",
    "typescript": "~4.6.3",
    "webpack": "^5.72.0"
  }
}
